{
  "name": "@luncheon/esbuild-plugin-linaria",
  "version": "0.0.0",
  "description": "An unofficial and experimental esbuild plugin for Linaria.",
  "license": "WTFPL",
  "repository": "luncheon/esbuild-plugin-linaria",
  "keywords": [
    "esbuild",
    "linaria"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf index.js index.d.ts",
    "build": "npm run clean && npm run build:js && npm run build:dts && npm t",
    "build:js": "node src/build.js",
    "build:dts": "tsc -p src",
    "test": "cd test && npm t"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@linaria/babel": "^3.0.0-beta.0",
    "@linaria/shaker": "^3.0.0-beta.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.12",
    "@types/node": "^14.14.16",
    "esbuild": "^0.8.26",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
